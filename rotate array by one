*******************************************************************************/
#include <iostream>
using namespace std;
 void rotate1(int arr[], int n )
{
    int temp=arr[n-1];
    for(int i=n-2;i>=0;i--){
        arr[i+1]=arr[i];
    }
    arr[0]=temp;
}
    
int main(){
    int array[]={1,2,3,4,5,6};
    int n =6;
    cout<<rotate1(array,n);
}
