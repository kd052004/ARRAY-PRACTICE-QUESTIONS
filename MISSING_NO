#include <iostream>
using namespace std;
int missing_no(int arr[], int  n ){
    int sum=0;
    for(int i=0;i<n;i++){
        sum+=arr[i];
    }
    int ans= n*(n+1)/2;
    return ans-sum;
}
int main()
{
    int array[]={1,2,3,5,6};
    int n=6;
    cout<< missing_no(array,n);
    

    return 0;
}
